{
  "name": "secret-sync",
  "version": "1.0.0",
  "private": true,
  "description": "Sync secrets across multiple repositories",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json",
    "eslint": "eslint . --ext .js,.cjs,.ts",
    "eslint:fix": "npm run eslint -- --fix",
    "lint": "tsc --noEmit && npm run eslint",
    "local": "cross-env NODE_OPTIONS=--openssl-legacy-provider ncc run src/main.ts",
    "package": "ncc build src/main.ts",
    "package:17": "cross-env NODE_OPTIONS=--openssl-legacy-provider npm run package",
    "release": "npm run package:17 && git add -f dist/",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage --config=jest.config.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xt0rted/secret-sync.git"
  },
  "keywords": [
    "actions",
    "github",
    "management",
    "security",
    "secrets"
  ],
  "author": "xt0rted",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/github": "^5.0.0",
    "@github/memoize": "^1.0.2",
    "@octokit/plugin-retry": "^3.0.9",
    "@octokit/plugin-throttling": "^3.5.2",
    "js-yaml": "^4.1.0",
    "tweetsodium": "^0.0.5"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.1",
    "@types/jest": "^27.4.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@vercel/ncc": "^0.33.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "eslint": "^8.8.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-unicorn": "^40.1.0",
    "jest": "^27.4.7",
    "jest-circus": "^27.5.0",
    "nock": "^13.2.2",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  }
}
